<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:AICalories.Views"
             x:Class="AICalories.Views.HomePage"
             BarBackgroundColor="White"
             BarTextColor="Black"
             Title="">
    
        <TabbedPage.ToolbarItems>
            <ToolbarItem Text="Menu" IconImageSource="menu.png" Clicked="OnMenuButtonClicked" />
        </TabbedPage.ToolbarItems>
    
        <NavigationPage Title="Photo">
            <x:Arguments>
                <local:PhotoSelectionPage />
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="History">
            <x:Arguments>
                <local:HistoryPage />
            </x:Arguments>
        </NavigationPage>
    
</TabbedPage>
